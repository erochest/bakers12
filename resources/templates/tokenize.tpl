<apply template='wrap'>
  <bind tag='pagetitle'>Bakers12 Tokenizer</bind>
  <bind tag='pagepostscripts'>
    <ratioarray/>
    <freqarray/>
  </bind>
      <div id='ratiograph'></div>
      <div id='freqgraph'></div>
      <table>
        <thead>
          <tr>
            <th>Token</th>
            <th>Raw</th>
            <th>Source</th>
            <th>Offset</th>
            <th>Length</th>
            <th>Running Type-to-Token Ratio</th>
          </thead>
        <tbody>
          <tokens>
            <tr>
              <td><token /></td>
              <td><raw /></td>
              <td><source /></td>
              <td><offset /></td>
              <td><length /></td>
              <td><ratio /></td>
            </tr>
          </tokens>
        </tbody>
      </table>
</apply>
