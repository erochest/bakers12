<html>
  <head>
    <title>Bakers12 Tokenizer</title>
    <link rel="stylesheet" type="text/css" href="screen.css"/>
  </head>
  <body>
    <div id="content">
      <h1>Bakers12 Tokenizer</h1>
      <table>
        <thead>
          <tr>
            <th>Token</th>
            <th>Raw</th>
            <th>Source</th>
            <th>Offset</th>
            <th>Length</th>
            <th>Running Type-to-Token Ratio</th>
          </thead>
        <tbody>
          <tokens>
            <tr>
              <td><token /></td>
              <td><raw /></td>
              <td><source /></td>
              <td><offset /></td>
              <td><length /></td>
              <td><ratio /></td>
            </tr>
          </tokens>
        </tbody>
      </table>
    </div>
  </body>
</html>
